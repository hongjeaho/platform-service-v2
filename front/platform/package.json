    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "orval": "^8.0.0-rc.2",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "storybook": "^8.4.7",
